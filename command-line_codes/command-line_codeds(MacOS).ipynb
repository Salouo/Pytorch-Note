   "source": [
    "## 3. Go up levers in the directory structure"
   ]
  },
  {
   "cell_type": "raw",
   "id": "2b1c6bdb-99ac-4031-b5ef-713ccee2c093",
   "metadata": {},
   "source": [
    "# go up one level\n",
    "cd ..\n",
    "\n",
    "# go up several level\n",
    "cd ../../\n",
    "\n",
    "# go up to base\n",
    "cd ~"
   ]
